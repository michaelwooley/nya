{
  "version": "0.1.0",
  "license": "../LICENSE",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "etl",
  "author": "Michael Wooley",
  "module": "dist/etl.esm.js",
  "size-limit": [
    {
      "path": "dist/etl.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/etl.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-document-nodes": "2.3.12",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-resolvers": "2.7.12",
    "@jest/globals": "^29.3.1",
    "@octokit/graphql-schema": "^12.41.1",
    "@size-limit/preset-small-lib": "^8.1.1",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "husky": "^8.0.3",
    "size-limit": "^8.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@octokit/graphql": "^5.0.4",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.6.0"
  }
}
